{
  "name": "temporal-cognition-openclaw",
  "version": "1.1.0",
  "description": "Temporal Cognition Module - OpenClaw plugin for AI temporal awareness, task estimation, priority scheduling, and 24/7 cycle management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "hooks",
    "openclaw.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/register.js || true"
  },
  "keywords": ["openclaw", "openclaw-plugin", "temporal", "ai", "agent"],
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "^0.32.0",
    "date-fns": "^3.3.0",
    "date-fns-tz": "^2.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0"
  },
  "openclaw": {
    "plugin": true,
    "slot": "tool",
    "manifest": "openclaw.plugin.json",
    "autoRegister": true
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
